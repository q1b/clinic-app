{
  "name": "clinic-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "generate": "drizzle-kit generate:sqlite --out ./drizzle/migrations --breakpoints --schema=./src/lib/server/db/schema/index.ts",
    "migrate": "tsx drizzle/migrate",
    "studio": "drizzle-kit studio --port 3333",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@js-temporal/polyfill": "^0.4.4",
    "@libsql/client": "^0.3.4",
    "@lucia-auth/adapter-session-redis": "^2.1.1",
    "@lucia-auth/adapter-sqlite": "^2.0.0",
    "@lucia-auth/oauth": "^3.3.0",
    "@sveltejs/kit": "^1.20.4",
    "@tailwindcss/forms": "^0.5.6",
    "@types/node": "^20.4.0",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@upstash/redis": "^1.22.0",
    "autoprefixer": "^10.4.14",
    "bits-ui": "^0.5.7",
    "clsx": "^2.0.0",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.19.13",
    "drizzle-orm": "^0.28.6",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "libphonenumber-js": "^1.10.44",
    "lucia": "^2.7.0",
    "lucide-svelte": "^0.279.0",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "radix-icons-svelte": "^1.2.1",
    "svelte": "^4.2.1",
    "svelte-adapter-azure-swa": "^0.19.0",
    "svelte-check": "^3.5.2",
    "tailwind-merge": "^1.14.0",
    "tailwind-variants": "^0.1.14",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.4.1",
    "tsx": "^3.13.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  },
  "type": "module"
}
