{
	"name": "v2",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"migrate": "tsx drizzle/migrate",
		"generate": "drizzle-kit generate:sqlite",
		"push": "drizzle-kit push:sqlite",
		"studio": "drizzle-kit studio --port 3333",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"pages:dev": "wrangler pages dev --compatibility-date=2023-11-15 --proxy 5173 -- npm run dev",
		"pages:deploy": "NODE_VERSION=16 npm run build && wrangler pages deploy .svelte-kit/cloudflare",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"gen:pwa-assets": "pwa-assets-generator"
	},
